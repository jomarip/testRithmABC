{
  "name": "rithm-art-bound-collection",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --coverage",
    "check-format": "prettier --check ./src",
    "format": "prettier  src \"*/**.{js.jsx,ts,tsx,css.md}\" --check --ignore-path .gitignore --write",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier \"**/*.(md|json|yml)\" --ignore-path .gitignore --check",
    "postinstall": "typechain  --target ethers-v6 --out-dir src/abi/abi-types 'src/abi/*.json'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.2",
    "@reduxjs/toolkit": "1.9.5",
    "@tanstack/react-table": "^8.9.2",
    "@tanstack/table-core": "^8.9.2",
    "@typechain/ethers-v6": "^0.5.0",
    "@types/lodash": "^4.14.199",
    "@types/react-redux": "^7.1.25",
    "@web3-react/core": "^8.2.2",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.2.9",
    "ethers": "^6.7.1",
    "formik": "^2.4.2",
    "history": "^5.3.0",
    "i18next": "^23.3.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "moralis": "^2.23.1",
    "precise-math": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-redux": "8.1.1",
    "react-router-dom": "^6.14.2",
    "react-toastify": "^9.1.3",
    "redux-saga": "^1.2.3",
    "rxjs": "^7.8.1",
    "typechain": "^8.3.1",
    "vite-plugin-svgr": "^3.2.0",
    "wagmi": "^1.4.2",
    "web3": "^4.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.4.7",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "license": "MIT"
}
